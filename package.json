    "typeorm": "^0.3.17",
    "uuidv4": "^6.2.13"
  },
  "devDependencies": {
    "@nestjs/cli": "^9.5.0",
    "@nestjs/schematics": "^9.0.0",
    "@nestjs/testing": "^9.0.0",
    "@types/bcryptjs": "^2.4.6",
    "@types/cache-manager-memcached-store": "^3.0.3",
    "@types/express": "^4.17.13",
    "@types/faker": "^6.6.9",
    "@types/jest": "29.2.4",
    "@types/json2csv": "^5.0.6",
    "@types/memcached": "^2.2.10",
    "@types/multer": "^1.4.10",
