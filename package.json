  "scripts": {
    "typeorm": "ts-node ./node_modules/typeorm/cli",
    "build": "rimraf dist && nest build",
    "start": "node dist/src/main",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start:dev": "NODE_ENV=dev nest start --debug --watch",
    "start:test": "NODE_ENV=testing nest start --debug --watch",
    "start:staging": "NODE_ENV=staging nest start --debug --watch",
    "start:prod": "NODE_ENV=prod nest start --debug --watch",
    "deploy": "npm ci && npm run build && npm run start",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "NODE_ENV=jest node_modules/.bin/jest test --detectOpenHandles --onlyChanged"
  },
  "dependencies": {
    "@nestjs/cache-manager": "^2.1.1",
    "@nestjs/cli": "^9.5.0",
    "@nestjs/common": "^9.0.0",
    "@nestjs/config": "^3.0.0",
    "@nestjs/core": "^9.0.0",
    "@nestjs/jwt": "^10.1.0",
    "@nestjs/mapped-types": "*",
    "@nestjs/passport": "^10.0.2",
    "@nestjs/platform-express": "^9.0.0",
    "@nestjs/typeorm": "^10.0.0",
    "bcryptjs": "^2.4.3",
    "class-transformer": "^0.5.1",
    "dotenv": "^16.3.1",
    "faker": "^5.5.3",
    "flat": "^6.0.1",
